{
  "name": "render-as-you-fetch",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://abeplays.github.io/devfinder",
  "dependencies": {
    "@craco/craco": "^5.9.0",
    "@types/node": "^12.20.25",
    "@types/react": "^17.0.21",
